<!-- Add you custom twig html here -->
<div class="wizard__before">
  <!-- Insert any block with description here, optional information -->
</div>
<div class="wizard">
  <div class="wizard__header-wrapper">
    <div class="wizard__header">
      <h2 class="wizard__header__title">{{ title }}</h2>
      <div class="wizard__header__content"><p>In case you're new here or don't exactly know what service is good for you, this wizard is meant to guide you through the selection process.</p></div>
    </div>
  </div>
  <div class="wizard__content">
    <div class="wizard__selections">
      {% for key,value in data %}
        <div class="wizard__selection">
          <a class="wizard-button button--action icon--arrow-right" data-tid="{{ value.tid }}">{{ value.name }}</a>
        </div>
      {% endfor %}
    </div>
    <div class="wizard__services"></div>
    <noscript>Unfortunately, wizard requires javascript to work.</noscript>
  </div>
  <div class="wizard__footer">
    <div class="wizard__footer__content"></div>
  </div>
</div>
<div class="wizard__after">
  <!-- Insert any block with description here, maybe feedback form? -->
</div>