{
    "name": "universityofhelsinki/researchdataservicechannel",
    "description": "Datasupport website with Drupal 8",
    "type": "project",
    "license": "proprietary",
    "homepage": "https://github.com/universityofhelsinki/researchdataservicechannel",
    "authors": [
        {
            "name": "Druid.fi",
            "email": "info@druid.fi"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "cweagans/composer-patches": "^1.6.5",
        "drupal/admin_toolbar": "^2.0",
        "drupal/adminimal_admin_toolbar": "^1.9",
        "drupal/adminimal_theme": "^1.3",
        "drupal/better_exposed_filters": "^3.0@alpha",
        "drupal/core-composer-scaffold": "^8.8",
        "drupal/core-recommended": "^8.8",
        "drupal/ds": "3.6.0",
        "drupal/externalauth": "^1.2",
        "drupal/fico": "1.0",
        "drupal/google_analytics": "2.4",
        "drupal/libraries": "3.x-dev",
        "drupal/pathauto": "^1.6",
        "drupal/restui": "^1.17",
        "drupal/samlauth": "^2.0@alpha",
        "drupal/stage_file_proxy": "^1.0",
        "drupal/system_status": "^2.8",
        "drupal/token": "^1.5",
        "drupal/webform": "5.13.0",
        "drush/drush": "^10.2",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "drupal-scaffold": {
            "allowed-packages": [
                "drupal/core"
            ],
            "locations": {
                "web-root": "./public"
            },
            "file-mapping": {
                "[project-root]/.editorconfig": false
            }
        },
        "installer-paths": {
            "public/core": ["type:drupal-core"],
            "public/libraries/{$name}": ["type:drupal-library"],
            "public/modules/contrib/{$name}": ["type:drupal-module"],
            "public/modules/custom/{$name}": ["type:drupal-module-custom"],
            "public/profiles/{$name}": ["type:drupal-profile"],
            "public/themes/contrib/{$name}": ["type:drupal-theme"],
            "public/themes/custom/{$name}": ["type:drupal-theme-custom"],
            "drush/{$name}": ["type:drupal-drush"]
        },
        "patches": {
            "drupal/samlauth": {
                "https://www.drupal.org/i/2860473": "https://www.drupal.org/files/issues/2860473-11.patch"
            },
            "onelogin/php-saml": {
                "deprecations": "https://patch-diff.githubusercontent.com/raw/onelogin/php-saml/pull/333.patch"
            }
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "config": {
        "platform": {
            "php": "7.3.10",
            "ext-mcrypt": "1.0.0"
        },
        "sort-packages": true
    }
}
